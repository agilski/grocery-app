// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id         String     @id @default(cuid())
  name       String     @default("")
  email      String     @unique @default("")
  password   String
  shopping   Shopping[] @relation("Shopping_user")
  privileged Boolean    @default(false)
}

model Shopping {
  id           String    @id @default(cuid())
  shoppingDate DateTime?
  status       String?   @default("open")
  user         User[]    @relation("Shopping_user")
  item         Item[]    @relation("Item_shopping")
}

model Item {
  id              String    @id @default(cuid())
  name            String    @default("")
  image_filesize  Int?
  image_extension String?
  image_width     Int?
  image_height    Int?
  image_mode      String?
  image_id        String?
  price           Int?
  shopping        Shopping? @relation("Item_shopping", fields: [shoppingId], references: [id])
  shoppingId      String?   @map("shopping")

  @@index([shoppingId])
}